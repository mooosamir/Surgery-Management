<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="document_list_tree_view" model="ir.ui.view">
            <field name="name">document.list.tree.view</field>
            <field name="model">document.list</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="document_list_action" model="ir.actions.act_window">
            <field name="name">Document List</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">document.list</field>
            <field name="view_mode">tree</field>
        </record>



    </data>
</odoo>